        "prefix": "NASMifloop",
        "body": [
        "SECTION .data\r",
        "\tstartMessage: db \"Starting program\",0ah,0\r",
        "\tprompt db \"Enter number:\",0ah,0\r",
        "\tlessThan db \"Less than 5\",0ah,0\r",
        "\tmoreThan db \"More than 5\",0ah,0\r",
        "\tequal: db \"Equal to 5\",0ah,0\r",
        "\tendMessage: db \"Ending program\",0ah,0\r",
        "\r",
        "SECTION .text\r",
        "\tglobal _main \r",
        "\textern _scanf   \r",
        "\textern _printf\r",
        "\r",
        "_main:\r",
        "\tjmp L1 \r",
        "\tjmp L2\r",
        "\tjmp End\r",
        "\r",
        "L1:\r",
        "\tpush startMessage\r",
        "\tcall _printf\r",
        "\tadd esp,4\r",
        "L2:\r",
        "\tmov eax, 5\r",
        "\r",
        "L3:\r",
        "\tcmp eax,5\r",
        "\tjg More\r",
        "\tjl Less\r",
        "\tje Equal\r",
        "\r",
        "More:\r",
        "\tpush moreThan\r",
        "\tcall _printf\r",
        "\tadd esp,4 \r",
        "Less:\r",
        "\tpush lessThan\r",
        "\tcall _printf\r",
        "\tadd esp,4\r",
        "\r",
        "Equal:\r",
        "\tpush equal\r",
        "\tcall _printf\r",
        "\tadd esp,4\r",
        "\r",
        "\r",
        "End:\r",
        "\tpush endMessage\r",
        "\tcall _printf\r",
        "\tadd esp,4\r",
        "\tret\r"
