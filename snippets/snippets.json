        "body":[
        ";Simple for loop program. It prints out the numbers from 1->12",
        "SECTION .data",
        "\tmessage1: db \"Starting loop\",0ah, 0",
        "\tmessage2: db \"Ending loop\", 0ah,0",
        "\tformat: db \"%d\", 0ah,0",
        "\tdata: db 0",
        "SECTION .texte",
        "\tglobal _main ",
        "\textern _printf\n",
        "_main:",
        "\tpush message1",
        "\tcall _printf",
        "\tadd esp,4",
        "\tmov cx, 12\n",
        "startloop:",
        "\tinc byte [data]",
        "\tcmp cx, 0",
        "\tjz endofloop",
        "\tpush cx\n",
        "loopy:",
        "\tmov eax,[data]",
        "\tpush eax",
        "\tpush format",
        "\tcall _printf",
        "\tadd esp,8",
        "\tpop cx",
        "\tdec cx",
        "\tjmp startloop\n",
        "endofloop:",
        "\tpush message2",
        "\tcall _printf",
        "\tadd esp,4",
        "\tret"
        ],
        "description:":"Basic for loop in nasm x86"
