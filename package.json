{
    "name": "nasm",
    "author": {
        "email": "l.irsigler64@gmail.com",
        "name": "Lucian Irsigler"
    },
    "publisher": "LucianIrsigler",
    "displayName": "nasm x86 syntax highlighting",
    "description": "syntax highlighting for nasm x86 assembly",
    "version": "1.1.11",
    "icon": "icon/icon.png",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    
    "repository": {
        "type": "git",
        "url": "https://github.com/CopyandPaste-png/nasm-x86-syntax-highlighting.git"
    },
    "license": "MIT",
    
    "keywords": [
        "asm",
        "nasm",
        "assembly",
        "NASM",
        "x86"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "nasm",
                "path": "./snippets/snippets.json"
            }
        ],
        "languages": [
            {
                "id": "nasm",
                "aliases": [
                    "nasm x86 assembly",
                    "nasm"
                ],
                "extensions": [
                    ".asm",
                    ".nasm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nasm",
                "scopeName": "source.asm",
                "path": "./syntaxes/nasm.tmLanguage.json"
            }
        ]}
}